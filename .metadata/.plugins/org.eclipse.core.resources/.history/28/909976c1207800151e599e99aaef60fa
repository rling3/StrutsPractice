package com.tutorialspoint.struts2;

public class HelloWorldAction extends ActionSupport{
	   private String name;

	   public String execute() throws Exception {
	      return "success";
	   }
	   
	   public String getName() {
	      return name;
	   }

	   public void setName(String name) {
	      this.name = name;
	   }
}